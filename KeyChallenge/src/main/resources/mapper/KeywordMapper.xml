<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="model.dao.KeywordMapper">
    <select id="getKeyword" resultType="java.lang.String" parameterType="java.lang.String">
        SELECT kw FROM KEYWORD WHERE idx = #{idx}
    </select>
</mapper>
